<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>Landmark Remark</title>
	<!-- Google font -->
	<link href="https://fonts.googleapis.com/css?family=Open+Sans" rel="stylesheet">
	@Styles.Render("~/bundles/styles")
</head>
<body>
	<!-- Renders the app base component server side, and provides .NET session user details -->
	@Html.React("AppBase", new
	{
		user = System.Web.HttpContext.Current.Session["user"]
	})

	<!-- Jquery -->
	<script src="https://code.jquery.com/jquery-3.3.1.min.js"
			integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="
			crossorigin="anonymous"></script>

	<!-- Google maps -->
	<script src="http://maps.google.com/maps/api/js?key=AIzaSyBnG2wcWi0MrBxd3wTtNCKTau-xHD_B324" type="text/javascript"></script>

	<!-- React -->
	<script crossorigin src="https://unpkg.com/react@16/umd/react.production.min.js"></script>
	<script crossorigin src="https://unpkg.com/react-dom@16/umd/react-dom.production.min.js"></script>

	@Scripts.Render("~/bundles/util")
	<!-- My react components -->
	@Scripts.Render("~/bundles/components")


	@Html.ReactInitJavaScript()
</body>
</html>